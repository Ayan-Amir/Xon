{
  "name": "xon-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "/app",
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "scripts": {
    "dev": "vite --port 3000",
    "generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js",
    "build": "yarn generate-meta-tag && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-folder": "node generate-folder.cjs",
    "make-page-directory": "yarn generate-folder"
  },
  "dependencies": {
    "@blocknote/core": "^0.9.3",
    "@blocknote/react": "^0.9.3",
    "@headlessui/react": "^1.7.17",
    "@react-oauth/google": "^0.11.1",
    "@sentry/react": "^7.69.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query": "^4.35.3",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.6.2",
    "axios": "^1.5.0",
    "change-object-case": "^0.2.1",
    "formik": "^2.4.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-cache-buster": "^0.1.8",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.3.1",
    "react-nice-avatar": "^1.4.1",
    "react-router-dom": "^6.16.0",
    "tailwind-scrollbar": "^3.0.5",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
